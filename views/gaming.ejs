app.get('/gethome', async (req, res) => {
    try {
        const homedata = await addlink.find({
            optionOption: "Home",
            approved: true
        }).sort({ createdAt: -1 });
        res.json({ homedata });
    } catch (err) {
        console.error(err);
        res.status(500).json({ error: 'Internal server error' });
    }
});